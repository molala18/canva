pipeline {
    agent any

    stages {
        stage('Greet') {
            steps {
                echo 'Hello from Jenkins!'
            }
        }

        stage('Date') {
            steps {
                sh 'date'
            }
        }

        stage('Goodbye') {
            steps {
                echo 'Goodbye from Jenkins lala to see poll!'
            }
        }
    }
}
